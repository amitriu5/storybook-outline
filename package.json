{
  "name": "storybook-addon-outline",
  "version": "1.0.0",
  "description": "A storybook addon for visually debugging CSS layout and alignment",
  "scripts": {
    "clean": "rimraf ./dist",
    "build": "babel ./src --out-dir ./dist",
    "prepublish": "npm run clean && npm run build",
    "test": "echo \"Error: no test specified\" && exit 1",
    "storybook": "start-storybook -p 6006",
    "start": "concurrently \"npm run storybook\" \"npm run build -- --watch\"",
    "build-storybook": "build-storybook"
  },
  "storybook": {
    "displayName": "Outline",
    "unsupportedFrameworks": ["react-native"]
  },
  "keywords": [
    "storybook-addons",
    "outline",
    "css",
    "layout",
    "debug"
  ],
  "author": "winkerVSbecks",
  "license": "MIT",
  "main": "dist/preset.js",
  "files": [
    "dist/**/*",
    "README.md",
    "*.js"
  ],
  "devDependencies": {
    "@babel/cli": "^7.12.1",
    "@babel/core": "^7.12.3",
    "@babel/preset-env": "^7.12.1",
    "@babel/preset-react": "^7.12.5",
    "@storybook/addon-actions": "next",
    "@storybook/addon-essentials": "next",
    "@storybook/react": "next",
    "babel-loader": "^8.1.0",
    "concurrently": "^5.3.0",
    "react-is": "^17.0.1",
    "rimraf": "^3.0.2"
  },
  "dependencies": {
    "@storybook/addons": "^6.1.0-beta.6",
    "react": "^17.0.1",
    "react-dom": "^17.0.1"
  },
  "peerDependencies": {
    "@storybook/addons": "next",
    "react": "^17.0.1",
    "react-dom": "^17.0.1"
  }
}
